{
  "name": "Tagboard Search",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Redirect searches for hashtags to Tagboard.com, and linkify hashtags around the web.",
  "icons": {  "16": "icon16.png",
              "48": "icon48.png",
              "128": "icon128.png" },
  "background": { "scripts": ["background.js"] },
  "permissions": ["webNavigation", "tabs", "*://*/*"],
  "options_page": "options.html"
}
